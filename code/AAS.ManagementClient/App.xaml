            <TextBox Style="{StaticResource TextBox}" 
                          Text="{TemplateBinding Property=Text}"
                          Tag="{TemplateBinding Property=Tag}"
                            Validation.ErrorTemplate="{TemplateBinding Property=Validation.ErrorTemplate}"
                     >
                <i:Interaction.Triggers>
